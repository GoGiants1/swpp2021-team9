    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.4",
    "konva": "~8.3.4",
    "lamejs": "1.2.0",
    "lint-staged": "10.3.0",
    "node-plop": "0.26.2",
    "peaks.js": "~2.0.0",
    "plop": "2.7.4",
    "prettier": "2.3.2",
    "react": "18.2.0",
    "react-app-polyfill": "2.0.0",
    "react-dom": "18.2.0",
    "react-easy-crop": "3.5.3",
    "react-helmet-async": "1.0.9",
    "react-i18next": "11.11.1",
    "react-image-crop": "9.0.5",
    "react-media-recorder": "1.6.2",
    "react-player": "2.9.0",
    "react-redux": "8.0.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
