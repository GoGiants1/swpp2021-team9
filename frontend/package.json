    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "10.0.0",
    "typesafe-actions": "5.1.0",
    "typescript": "4.2.2",
    "waveform-data": "~4.3.0",
    "wavesurfer.js": "5.2.0",
